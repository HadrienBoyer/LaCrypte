{
  "name": "lacrypte",
  "version": "1.2.9",
  "description": "Encrypted HTML5/Node.JS instant chat",
  "main": "server/server.js",
  "preferGlobal": true,
  "private": false,
  "scripts": {
    "test": "echo \"No tests written yet\" && exit 0",
    "build": "npm update && npm outdated && npm run test:lint && npx rollup -c rollup.config.js && npm run build:minify && npm run build:cleanup",
    "build:ci": "npm run test:lint && npx rollup -c rollup.config.js && npm run build:minify && npm run build:cleanup",
    "build:minify": "uglifyjs client/public/js/lacrypte.js --source-map -o client/public/js/lacrypte.min.js",
    "build:cleanup": "(rm client/public/js/lacrypte.js || del client\\public\\js\\lacrypte.js)",
    "test:lint": "eslint ./client/source/**/*.js ./server/*.js",
    "start": "node ./server/server.js",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start"
  },
  "keywords": [
    "lacrypte",
    "chat",
    "crypto-js",
    "AES",
    "secure",
    "html5",
    "encryption",
    "privacy",
    "e2ee"
  ],
  "author": "HadrienBoyer <HadrienBoyer@GitHub>",
  "contributors": [
    {
      "name": "HadrienBoyer",
      "email": "hello@hadrien.co"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/HadrienBoyer/LaCrypte.git"
  },
  "bin": "./server/server.js",
  "dependencies": {
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "^4.3.0",
    "@capacitor/dialog": "^4.0.1",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "serve": "^14.0.1",
    "socket.io": "^4.5.2"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "devDependencies": {
    "@capacitor/cli": "^4.3.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "cordova-res": "0.15.4",
    "crypto-js": "^4.1.1",
    "eslint": "^8.24.0",
    "rollup": "^2.79.1",
    "uglify-js": "^3.17.2"
  }
}
